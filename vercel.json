{
  "version": 2,
  "buildCommand": "cd server && npm install",
  "functions": {
    "server/index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/mcp",
      "destination": "/server/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/server/index.js"
    }
  ]
}
